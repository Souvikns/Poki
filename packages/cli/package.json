{
  "name": "poki",
  "version": "0.1.0",
  "license": "MIT",
  "bin": "dist/cli.js",
  "types": "dist/cli.d.ts",
  "author": {
    "name": "Souvik De",
    "email": "souvikde.ns@gmail.com",
    "url": "https://github.com/Souvikns"
  },
  "engines": {
    "node": ">=10"
  },
  "homepage": "https://github.com/Souvikns/Poki#readme",
  "scripts": {
    "build": "tsc && copyfiles -f ./source/lib/data/sprites/pokemon/*.png dist/lib/data/sprites/pokemon",
    "start": "npm run build && node dist/cli.js",
    "pretest": "npm run build",
    "test": "jest --coverage && codecov",
    "test:watch": "jest --watch",
    "prepublish": "npm run build"
  },
  "files": [
    "dist",
    "static"
  ],
  "dependencies": {
    "@reduxjs/toolkit": "^1.5.1",
    "@types/ink-select-input": "^3.0.0",
    "@types/node": "^14.14.41",
    "commander": "^7.2.0",
    "ink": "^3.0.8",
    "ink-big-text": "^1.2.0",
    "ink-gradient": "^2.0.0",
    "ink-link": "^2.0.0",
    "ink-select-input": "^4.2.0",
    "ink-text-input": "^4.0.1",
    "lodash": "^4.17.21",
    "meow": "^9.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.5.0"
  },
  "devDependencies": {
    "@sindresorhus/tsconfig": "^0.9.0",
    "@types/chalk": "^2.2.0",
    "@types/ink-big-text": "^1.2.0",
    "@types/ink-gradient": "^2.0.1",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.168",
    "@types/react": "^17.0.2",
    "chalk": "^4.1.0",
    "codecov": "^3.8.1",
    "copyfiles": "^2.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "ink-testing-library": "^2.1.0",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.3",
    "typescript": "^4.2.3"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/test/setupTests.ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/tests/.*|(\\.|/)(test|spec))\\.(tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  }
}
